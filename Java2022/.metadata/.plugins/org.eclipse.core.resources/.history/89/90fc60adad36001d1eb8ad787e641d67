public class switchDemo {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
        
		char grade = 'A';
		
		switch(grade) {
		   
		   case 'A' :
			   System.out.println("Mükemmel : Geçtiniz!");
			   break;
			
		   case 'B' :
			   System.out.println("Çok Güzel : Geçtiniz!");
 
		}
	}

}
