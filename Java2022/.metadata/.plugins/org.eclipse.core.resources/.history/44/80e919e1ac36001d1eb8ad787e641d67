
public class recapDemo1 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
        
		int sayi1 = 30;
		int sayi2 = 25;
		int sayi3 = 2;
		
		int enBuyuk = sayi1;
		
		if(enBuyuk < sayi2) {
			
			enBuyuk = sayi2;
		}
		if(enBuyuk < sayi3) {
			
			enBuyuk = sayi3;
		}
	}

}
