public class Conditionals {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
        
		int sayi = 14;
		
		if(sayi < 20) {
			
			System.out.println("Sayı 20 den küçüktür.");
		}
		
        if(sayi < 15) {
			
			System.out.println("Sayı 15 den küçüktür.");
		}
	}

}
