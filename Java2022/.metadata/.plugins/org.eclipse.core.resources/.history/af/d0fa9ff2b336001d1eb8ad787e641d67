public class multiDimensionalArrayDemo {

	public static void main(String[] args) {
		String[][] sehirler = new String[3][3];
		
		sehirler[0][0] = "İstanbul";
		sehirler[0][1] = "Samsun";
		sehirler[0][2] = "Niğde";
		sehirler[1][0] = "Çorum";
		sehirler[1][1] = "Kırklareli";
		sehirler[1][2] = "Ankara";
		sehirler[2][0] = "Muş";
        sehirler[2][1] = "Karaman";
		sehirler[2][2] = "Antalya";		



	}

}
