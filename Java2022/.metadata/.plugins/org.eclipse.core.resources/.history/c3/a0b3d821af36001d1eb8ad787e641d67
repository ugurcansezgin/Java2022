public class loopDemo {

	public static void main(String[] args) {
		// FOR
		
		for(int i=1; i<10; i++) {
			System.out.println(i);
		}
		
		System.out.println("For Döngüsü bitti!");

		int i = 1;
		// WHİLE
		
		while(i < 10) {
			System.out.println("While Döngüsü bitti!");

		}
		
		System.out.println("While Döngüsü bitti!");
	}

}
